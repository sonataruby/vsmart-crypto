</script>
<style type="text/css">
    header .header-main{
       
        background-repeat: no-repeat;
        background-position: top center;
        background-size: 100%;
        
    }

    div.header{
      position: fixed;
      width: 130px;
      left: 0;
      height: 100%;
      z-index: 8;
    }
    div.header li{
      border-bottom: 1px solid #ddd;
    }
    div.header li a{
      color: #FFF;
      
    }
    div.header li a i{
      display: block;
      font-size: 48px;
    }
    
main{
  padding-left: 130px;
  background-image: url(/dist/game/bg2.jpg);
  min-height: 100%;
  position: fixed;
  width: 100%;
  background-size: cover;

  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
 

}
@media (max-width: 500px) {
  div.header{
    display: none;
  }
  main{
    padding-left: 0px;
  }
  }
</style>

</head>
